{
    "name": "@danielytics/tg-analytics",
    "version": "1.4.6",
    "description": "Fork of Telegram Analytcis that makes it GDPR-compliant ",
    "homepage": "https://github.com/danielytics/tg-analytics#readme",
    "bugs": {
        "url": "https://github.com/danielytics/tg-analytics/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/danielytics/tg-analytics.git"
    },
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./lib/types/index.d.ts",
            "require": "./lib/cjs/index.cjs",
            "import": "./lib/es/index.js",
            "default": "./lib/cjs/index.cjs"
        }
    },
    "main": "dist/index.js",
    "types": "./lib/types/index.d.ts",
    "directories": {
        "test": "tests"
    },
    "files": [
        "lib",
        "dist"
    ],
    "scripts": {
        "build": "wasm-pack build ./src/wasm/human-proof --target web && vite build --mode script && vite build --mode package",
        "watch": "vite --watch",
        "serve": "http-server -p 8080 ./dist",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@telegram-apps/sdk": "^1.1.0",
        "@tonconnect/ui": "2.0.5",
        "http-server": "14.1.1"
    },
    "devDependencies": {
        "@twa-dev/types": "^7.0.0",
        "@types/es6-promise": "^3.3.0",
        "javascript-obfuscator": "^4.1.1",
        "terser": "^5.31.0",
        "typescript": "^5.4.5",
        "vite": "^5.2.12",
        "vite-plugin-dts": "^4.2.1",
        "vite-plugin-javascript-obfuscator": "^3.1.0",
        "vite-plugin-static-copy": "^1.0.6",
        "vite-tsconfig-paths": "^5.0.1",
        "wasm-pack": "^0.13.1"
    },
    "module": "./lib/cjs/index.cjs",
    "resolutions": {
        "@types/node": "18.11.9"
    }
}
